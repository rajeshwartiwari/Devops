pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Build started'
      }
    }
    stage('verify') {
      steps {
        echo 'Verify started'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploy'
      }
    }
    stage('End') {
      steps {
        echo 'End'
      }
    }
  }
}